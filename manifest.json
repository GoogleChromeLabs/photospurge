{
  "manifest_version": 3,
  "name": "Photos Purge",
  "version": "0.0.2",
  "author": "Clay Smith",
  "short_name": "Photos Purge",
  "default_locale": "en",
  "description": "Purge Google Photos.",
  "icons": {
    "19": "images/19.png",
    "32": "images/32.png",
    "38": "images/38.png",
    "36": "images/36.png",
    "64": "images/64.png",
    "128": "images/128.png"
  },
  "action": {
    "default_icon": {
      "19": "images/19.png",
      "38": "images/38.png"
    }
  },
  "content_scripts": [
    {
      "run_at": "document_start",
      "matches": [
        "https://photos.google.com/*"
      ],
      "exclude_matches": [
        "https://photos.google.com/albums/*"
      ],
      "js": [
        "contentscript.js",
        "third_party/materialize.min.js"
      ],
      "css": [
        "third_party/material.min.css"
      ]
    }
  ]
}
